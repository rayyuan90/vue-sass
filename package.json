{
  "name": "starter-vue-vuex-scss",
  "description": "A lightweight Vue.js starter including Vuex, Vue Router and Axios.",
  "version": "1.0.0",
  "main": "src/main.js",
  "license": "MIT",
  "author": "Quentin Raimbault <quentin.raimbault@gmail.com>",
  "sassLintConfig": ".sass-lint.yml",
  "repository": {
    "url": "https://github.com/QRaimbault/vue-js-starter-scss",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/QRaimbault/vue-js-starter-scss/issues",
    "email": "quentin.raimbault@gmail.com"
  },
  "homepage": "https://qraimbault.github.io/vue-js-starter-scss/#/",
  "keywords": [
    "vue",
    "vuejs",
    "starter",
    "vuex",
    "axios-vue",
    "sass",
    "scss",
    "vue-router",
    "vuejs2",
    "vue2",
    "jest",
    "scss-styles"
  ],
  "scripts": {
    "start": "cross-env NODE_ENV=development node bin/dev-server.js",
    "build": "cross-env NODE_ENV=production webpack",
    "serve": "cross-env NODE_ENV=production node bin/spa-server.js",
    "lint-js": "eslint --ext .js,.vue src",
    "lint-js-fix": "eslint --ext .js,.vue src --fix",
    "lint-scss": "sass-lint -c ./.sass-lint.yml",
    "unit": "jest"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "moduleNameMapper": {
      "\\.(css|jpg|jpeg|png|scss|sass|svg|eot|woff|woff2|ttf|otf|ico)$": "<rootDir>/__test__/empty-module.js"
    }
  },
  "dependencies": {
    "axios": "^0.18.0",
    "vue": "^2.5.16",
    "vue-analytics": "^5.12.2",
    "vue-cookie": "^1.1.4",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.51",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0-beta.51",
    "@babel/preset-env": "^7.0.0-beta.51",
    "babel-core": "^7.0.0-0",
    "babel-jest": "^23.2.0",
    "babel-loader": "^8.0.0-beta",
    "babel-preset-env": "^1.7.0",
    "copy-webpack-plugin": "^4.5.2",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.11",
    "eslint": "^4",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-vue": "^4.5.0",
    "express": "^4.16.3",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.2.0",
    "mini-css-extract-plugin": "^0.4.1",
    "node-sass": "^4.9.0",
    "opn": "^5.3.0",
    "optimize-css-assets-webpack-plugin": "^4.0.3",
    "regenerator-runtime": "^0.12.0",
    "resolve-url-loader": "^2.3.0",
    "sass-lint": "^1.12.1",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "sugarss": "^1.0.1",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "url-loader": "^1.0.1",
    "vue-eslint-parser": "^2.0.3",
    "vue-jest": "^2.6.0",
    "vue-loader": "^15.0.0-beta",
    "vue-template-compiler": "^2.5.16",
    "vue-test-utils": "^1.0.0-beta.11",
    "webpack": "^4.14.0",
    "webpack-cli": "^3.0.8",
    "webpack-serve": "^1.0.4"
  }
}
